{
  "name": "hafiportrait-web-dashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -H 0.0.0.0 -p ${PORT:-3000}",
    "build": "next build",
    "build:staging": "next build",
    "start": "next start -p $PORT",
    "lint": "next lint",
    "env:dev": "echo 'Manual env mode - edit .env.local directly'",
    "env:prod": "echo 'Manual env mode - edit .env.production directly'",
    "env:staging": "echo 'Manual env mode - edit .env.staging directly'",
    "env:status": "node scripts/env-status.js",
    "ws:start": "node scripts/start-websocket-server.js",
    "ws:dev": "nodemon scripts/start-websocket-server.js",
    "socketio:start": "node scripts/start-socketio-server.js",
    "socketio:dev": "nodemon scripts/start-socketio-server.js",
    "socketio:local": "node socketio-server.js",
    "socketio:local:dev": "nodemon socketio-server.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:status": "pm2 status",
    "dev:full": "concurrently \"pnpm run dev\" \"pnpm run ws:start\"",
    "dev:socketio": "concurrently \"pnpm run dev\" \"pnpm run socketio:start\"",
    "test:migration": "node scripts/test-socketio-migration.js",
    "test:socketio": "NEXT_PUBLIC_USE_SOCKETIO=true pnpm run dev:socketio",
    "test:websocket": "NEXT_PUBLIC_USE_SOCKETIO=false pnpm run dev:full",
    "cloudrun:build": "docker build -t hafiportrait .",
    "cloudrun:run": "docker run -p 8080:8080 hafiportrait",
    "install:all": "pnpm install && cd DSLR-System && pnpm install",
    "clean": "rm -rf node_modules pnpm-lock.yaml && cd DSLR-System && rm -rf node_modules pnpm-lock.yaml",
    "fresh-install": "pnpm run clean && pnpm run install:all",
    "koyeb:setup": "node scripts/setup-koyeb-deployment.js",
    "koyeb:quick": "bash scripts/quick-koyeb-setup.sh",
    "koyeb:deploy": "cd ws && koyeb app redeploy hafiportrait-websocket",
    "koyeb:logs": "koyeb app logs hafiportrait-websocket",
    "koyeb:status": "koyeb app get hafiportrait-websocket",
    "zeabur:setup": "node scripts/deploy-zeabur-socketio.js",
    "zeabur:migrate": "node scripts/migrate-to-zeabur-socketio.js",
    "zeabur:monitor": "node scripts/monitor-zeabur-socketio.js",
    "zeabur:fix": "node scripts/fix-zeabur-deployment.js",
    "zeabur:test": "cd zeabur-socketio && npm test",
    "zeabur:health": "cd zeabur-socketio && npm run health",
    "test:zeabur-integration": "node scripts/test-zeabur-integration.js",
    "health:zeabur": "node scripts/check-zeabur-health.js",
    "monitor:start": "node scripts/enhanced-health-monitor.js start",
    "monitor:check": "node scripts/enhanced-health-monitor.js check",
    "monitor:status": "node scripts/enhanced-health-monitor.js status",
    "monitor:system": "bash scripts/start-monitoring.sh",
    "alerts:send": "node scripts/alert-manager.js send",
    "alerts:history": "node scripts/alert-manager.js history",
    "alerts:stats": "node scripts/alert-manager.js stats",
    "ci:monitor": "node scripts/ci-monitoring-integration.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/s3-request-presigner": "^3.864.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
